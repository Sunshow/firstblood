<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.cms">
	<class name="VoteItem" table="admin_cms_voteitem">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="voteID" column="vote_id" not-null="true"/>
        <property name="item" not-null="true"/>
        <property name="num"/>
    </class>
</hibernate-mapping>