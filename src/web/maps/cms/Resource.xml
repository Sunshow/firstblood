<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.cms">
	<class name="Resource" table="admin_cms_resource">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="cateID" column="cate_id" not-null="true"/>
        <property name="name"/>
        <property name="path" not-null="true"/>
        <property name="createTime" column="create_time" not-null="true" update="false"/>
        <property name="valid" not-null="true"/>
    </class>
</hibernate-mapping>