<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.process">
	<class name="WorkProcess" table="admin_business_work_process">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="processId" column="process_id"/>
        <property name="processName" column="process_name"/>
    </class>
</hibernate-mapping>