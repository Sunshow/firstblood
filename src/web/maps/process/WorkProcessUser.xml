<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.process">
	<class name="WorkProcessUser" table="admin_business_work_process_user">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="processId" column="process_id"/>
        <property name="taskId" column="task_id"/>
        <property name="processUserType">
        	<column name="process_user_type"/>
			<type name="com.lehecai.admin.web.hibernate.usertype.IntegerBeanLabelUserType">
				<param name="targetClass">
					com.lehecai.admin.web.enums.ProcessUserType
				</param>
			</type>
        </property>
        <property name="name" column="name"/>
        <property name="operateId" column="operate_id"/>
        <property name="tel" column="tel"/>
        <property name="email" column="email"/>
        <property name="amount" column="amount"/>
        <property name="notifyTimes" column="notify_times"/>
        <property name="dutyDate" column="duty_date"/>
        <property name="notify" not-null="true"/>
        <property name="emailNotify" column="email_notify" not-null="true"/>
        <property name="smsNotify" column="sms_notify" not-null="true"/>
        <property name="messageNotify" column="message_notify" not-null="true"/>
    </class>
</hibernate-mapping>