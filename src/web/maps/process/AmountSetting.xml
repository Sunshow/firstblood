<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.process">
	<class name="AmountSetting" table="admin_business_work_process_amount_setting">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="processId" column="process_id"/>
        <property name="taskId" column="task_id"/>
        <property name="operateId" column="operate_id"/>
        <property name="cycleYear" column="cycle_year"/>
        <property name="cycleMonth" column="cycle_month"/>
        <property name="cycleDay" column="cycle_day"/>
        <property name="cycleAmount" column="cycle_amount"/>
        <property name="restAmount" column="rest_amount"/>
        <property name="beginTime" column="begin_time"/>
        <property name="endTime" column="end_time"/>
    </class>
</hibernate-mapping>