<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.link">
	<class name="FriendLink" table="admin_friend_link" dynamic-insert="true" dynamic-update="true">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="name" length="255" not-null="true"/>
 		<property name="url" length="4000" not-null="true"/>
        <property name="onIndex" not-null="true">
        	<column name="on_index"/>
        </property>
        <property name="highlight" not-null="true">
        </property>
		<property name="orderView" not-null="true">
			<column name="order_view"/>
		</property>
        <property name="openNew" not-null="true">
        	<column name="open_new"/>
        </property>
        <property name="valid" not-null="true"/>
        <property name="memo"/>
    </class>
</hibernate-mapping>