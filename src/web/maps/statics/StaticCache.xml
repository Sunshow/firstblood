<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.statics">
	<class name="StaticCache" table="admin_statics_staticcache">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="name"/>
        <property name="slug" unique="true"/>
        <property name="executeType">
        	<column name="execute_type" precision="22" scale="0"/>
			<type name="com.lehecai.admin.web.hibernate.usertype.IntegerBeanLabelUserType">
				<param name="targetClass">
					com.lehecai.admin.web.enums.ExecuteType
				</param>
			</type>
        </property>
        <property name="dataUrl" column="data_url" length="4000"/>
        <property name="templateType">
        	<column name="template_type" precision="22" scale="0"/>
			<type name="com.lehecai.admin.web.hibernate.usertype.IntegerBeanLabelUserType">
				<param name="targetClass">
					com.lehecai.admin.web.enums.TemplateType
				</param>
			</type>
        </property>
        <property name="templateUrl" column="template_url" length="4000"/>
        <property name="targetUrl" column="target_url" unique="true"/>
        <property name="valid"/>
        <property name="memo" length="4000"/>
        <property name="updateTime" column="update_time"/>
        <property name="userID" column="user_id"/>
        <property name="userName" column="user_name"/>
        <property name="stLevel" column="st_level"/>
        <property name="parentID" column="parent_id"/>
        <property name="orderView" column="order_view"/>
    </class>
</hibernate-mapping>