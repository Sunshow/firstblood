<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.statics">
	<class name="StaticCacheLayout" table="admin_static_cache_layout">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="name"/>
        <property name="valid"/>
        <property name="memo" length="4000"/>
        <property name="theLevel" column="the_level"/>
        <property name="parentId" column="parent_id"/>
        <property name="orderView" column="order_view"/>
    </class>
</hibernate-mapping>