<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.statics">
	<class name="StaticFragment" table="admin_static_fragment">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="fragmentName">
        	<column name="fragment_name" not-null="true" />
        </property>
        <property name="memo"/>
        
        <property name="orderValue">
        	<column name="order_value" not-null="true" />
        </property>
        <property name="targetUrl">
        	<column name="target_url" not-null="true" />
        </property>
        <property name="moduleId">
        	<column name="module_id" not-null="true" />
        </property>
    </class>
</hibernate-mapping>