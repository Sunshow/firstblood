<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.statics">
	<class name="DataConsolidationItem" table="admin_data_consolidation_item" dynamic-insert="true" dynamic-update="true">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="dataId" column="data_id"/>
        <property name="dataKey" column="data_key" />
        <property name="url" />
    </class>
</hibernate-mapping>