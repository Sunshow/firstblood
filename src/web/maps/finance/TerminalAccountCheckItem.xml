<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.finance">
	<class name="TerminalAccountCheckItem" table="admin_terminal_account_check_item">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="lotteryType">
        	<column name="lottery_type" precision="22" scale="0" not-null="true" />
			<type name="com.lehecai.admin.web.hibernate.usertype.IntegerBeanLabelUserType">
				<param name="targetClass">
					com.lehecai.core.lottery.LotteryType
				</param>
			</type>
        </property>
        <property name="terminalCompanyId">
        	<column name="terminal_company_id" not-null="true" />
        </property>
        <property name="terminalCompanyName">
        	<column name="terminal_company_name" not-null="true" />
        </property>
        <property name="accountCheckType">
        	<column name="account_check_type" not-null="true"/>
        </property>
        <property name="amountCheckDate">
        	<column name="amount_check_date"/>
        </property>
        <property name="terminalDrawMoney">
        	<column name="terminal_draw_money"/>
        </property>
        <property name="terminalPrizeMoney">
        	<column name="terminal_prize_money"/>
        </property>
        <property name="lehecaiDrawMoney">
        	<column name="lehecai_draw_money"/>
        </property>
        <property name="lehecaiPrizeMoney">
        	<column name="lehecai_prize_money"/>
        </property>
        <property name="drawMoneyDiff">
        	<column name="draw_money_diff"/>
        </property>
        <property name="drawDiffReason">
        	<column name="draw_diff_reason"/>
        </property>
        <property name="prizeMoneyDiff">
        	<column name="prize_money_diff"/>
        </property>
        <property name="prizeDiffReason">
        	<column name="prize_diff_reason"/>
        </property>
        <property name="commission">
        	<column name="commission"/>
        </property>
        <property name="memo">
        	<column name="memo"/>
        </property>
        <property name="userId">
        	<column name="user_id" not-null="true" />
        </property>
        <property name="createTime" update="false">
        	<column name="create_time"/>
        </property>
        <property name="enterAccount">
        	<column name="enter_account"/>
        </property>
    </class>
</hibernate-mapping>