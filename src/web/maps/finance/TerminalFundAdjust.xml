<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.finance">
	<class name="TerminalFundAdjust" table="admin_terminal_fund_adjust">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="userId">
        	<column name="user_id" not-null="true" />
        </property>
        <property name="createTime" update="false">
        	<column name="create_time" not-null="true"/>
        </property>
        <property name="amount">
        	<column name="amount" not-null="true"/>
        </property>
        <property name="payType">
        	<column name="pay_type" precision="22" scale="0" not-null="true" />
			<type name="com.lehecai.admin.web.hibernate.usertype.IntegerBeanLabelUserType">
				<param name="targetClass">
					com.lehecai.admin.web.domain.finance.PayType
				</param>
			</type>
        </property>
        <property name="reason">
        	<column name="reason"/>
        </property>
        <property name="terminalAccountCheckItemId">
        	<column name="terminal_account_check_item_id" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>