<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lehecai.admin.web.domain.agent">
	<class name="AgentLink" table="admin_agent_link">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="linkTypeId" column="link_type_id" not-null="true"/>
        <property name="url" length="4000"/>
        <property name="createTime" column="create_time" not-null="true" update="false"/>
        <property name="updateTime" column="update_time"/>
        <property name="valid" not-null="true"/>
    </class>
</hibernate-mapping>