<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.lehecai.admin.web.domain.business">
	<class name="SmsMailModel" table="admin_business_sms_mail_model">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="type" not-null="true">
        	<column name="type" precision="22" scale="0" not-null="true" />
			<type name="com.lehecai.admin.web.hibernate.usertype.IntegerBeanLabelUserType">
				<param name="targetClass">
					com.lehecai.admin.web.enums.ModelType
				</param>
			</type>
        </property>
         <property name="textType" not-null="true">
        	<column name="texttype" precision="22" scale="0" not-null="true" />
			<type name="com.lehecai.admin.web.hibernate.usertype.IntegerBeanLabelUserType">
				<param name="targetClass">
					com.lehecai.admin.web.enums.TextType
				</param>
			</type>
        </property>
        <property name="title" type="org.hibernate.type.StringClobType"/>
        <property name="content" type="org.hibernate.type.StringClobType"/>
        <property name="createTime" column="create_time" not-null="true" update="false"/>
        <property name="updateTime" column="update_time"/>
    </class>
</hibernate-mapping>